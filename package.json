    "ts-loader": "^4.0.1",
    "typescript": "2.7.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "dependencies": {
    "brotli": "^1.3.2",
    "console-to-terminal": "^1.0.1",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "http-proxy": "^1.16.2",
    "lodash": "^4.17.5",
    "yargs": "^11.0.0"
